{
  "expo": {
    "name": "HomeoFace",
    "description": "Dünyanın ilk ve tek tuzlarla yüz tanıma uygulaması",
    "slug": "HomeoFace",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0001",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.homeocure.homeoface"
    },
    "android": {
      "package": "com.homeocureinc.homeoface"
    }
  }
}